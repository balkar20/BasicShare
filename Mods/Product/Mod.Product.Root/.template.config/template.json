{
    "$schema": "https://json.schemastore.org/template",
    "author": "Vlad Balkarov",
    "name": "Mods For BasicShare",
    "description": "A project for creating a ModsRoot in BasicShare.",
    "identity": "Mod.Product.Root",
    "shortName": "modproductroot",
    "tags": {
      "language": "C#",
      "type": "project"
    },
    "symbols":{
        "copyrightName": {
          "type": "parameter",
          "defaultValue": "NewItem",
          "fileRename": "Product",
          "replaces":"Product"
        },
        "title": {
          "type": "parameter",
          "defaultValue": "Hello Web",
          "replaces":"Sayed Web"
        }
    },
    "sourceName": "Mod.Product.Root",
    "preferNameDirectory": true
  }