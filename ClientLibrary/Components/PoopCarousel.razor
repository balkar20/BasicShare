<MudCarousel Class="mud-width-full" Style="height:400px; margin-bottom: 50px" ShowArrows="@arrows" ShowBullets="@bullets" EnableSwipeGesture="@enableSwipeGesture" AutoCycle="@autocycle" TData="object">
    <MudCarouselItem Transition="transition" Color="@Color.Primary">
        <div class="d-flex justify-content-center" style="height:100%">
            <MudCard>
                <MudCardMedia Image="Bluewing_Anime_1.png" Height="200" />
                <MudCardContent>
                    <MudText Typo="Typo.h5">Old Paint</MudText>
                    <MudText Typo="Typo.body2">Old paint found on a stone house door.</MudText>
                    <MudText Typo="Typo.body2">This photo was taken in a small village in Istra Croatia.</MudText>
                </MudCardContent>
                <MudCardActions>
                    <MudButton Variant="Variant.Text" Color="Color.Primary">Share</MudButton>
                    <MudButton Variant="Variant.Text" Color="Color.Primary">Learn More</MudButton>
                </MudCardActions>
            </MudCard>
            @* <MudAvatar Class="mx-auto my-auto" Image="Bluewing_Anime_1.png" Size="@Size.Large"/> *@
        </div>
    </MudCarouselItem>
    <MudCarouselItem Transition="transition" Color="@Color.Secondary">
        <div class="d-flex justify-content-center" style="height:100%">
            <MudCard>
                <MudCardMedia Image="Bluewing_Anime_2.png" Height="200"/>
                <MudCardContent>
                    <MudText Typo="Typo.h5">Old Paint</MudText>
                    <MudText Typo="Typo.body2">Old paint found on a stone house door.</MudText>
                    <MudText Typo="Typo.body2">This photo was taken in a small village in Istra Croatia.</MudText>
                </MudCardContent>
                <MudCardActions>
                    <MudButton Variant="Variant.Text" Color="Color.Primary">Share</MudButton>
                    <MudButton Variant="Variant.Text" Color="Color.Primary">Learn More</MudButton>
                </MudCardActions>
            </MudCard>
            @* <MudAvatar Class="mx-auto my-auto" Image="Bluewing_Anime_2.png" Size="@Size.Large"/> *@
        </div>
    </MudCarouselItem>
    <MudCarouselItem  Transition="transition">
        <div class="d-flex  justify-content-center" style="height:100%">
            <MudCard>
                <MudCardMedia Image="Bluewing_Anime_3.png" Height="200"/>
                <MudCardContent>
                    <MudText Typo="Typo.h5">Old Paint</MudText>
                    <MudText Typo="Typo.body2">Old paint found on a stone house door.</MudText>
                    <MudText Typo="Typo.body2">This photo was taken in a small village in Istra Croatia.</MudText>
                </MudCardContent>
                <MudCardActions>
                    <MudButton Variant="Variant.Text" Color="Color.Primary">Share</MudButton>
                    <MudButton Variant="Variant.Text" Color="Color.Primary">Learn More</MudButton>
                </MudCardActions>
            </MudCard>
            @* <MudAvatar  Image="Bluewing_Anime_3.png" Size="@Size.Large" /> *@
        </div>
    </MudCarouselItem>
</MudCarousel>
